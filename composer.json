{
    "name": "oro/crm-task-bundle",
    "type": "symfony-bundle",
    "description": "Oro Task Bundle",
    "homepage": "https://github.com/oroinc/OroCRMTaskBundle.git",
    "keywords": ["Oro", "Task"],
    "license": "OSL-3.0",
    "authors": [
        {
            "name": "Oro, Inc",
            "homepage": "http://www.orocrm.com"
        }
    ],
    "require": {
        "php": "7.1.*",
        "oro/platform": "3.0.x-dev"
    },
    "autoload": {
        "psr-4": { "Oro\\Bundle\\TaskBundle\\": "" },
        "exclude-from-classmap": ["/Tests/"]
    },
    "repositories": {
        "oro": {
            "type": "composer",
            "url": "https://packagist.orocrm.com"
        },
        "asset-packagist": {
            "type": "composer",
            "url": "https://asset-packagist.orocrm.com"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
             "dev-3.0-beta": "3.0.x-dev"
        }
    }
}
